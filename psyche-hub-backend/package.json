{
  "name": "express-typescript-boilerplate",
  "version": "1.0.14",
  "description": "An Express boilerplate backend",
  "author": "Edwin Hernandez",
  "repository": "edwinhern/express-typescript-2024",
  "license": "MIT",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "build": "tsc && tsup",
    "start:dev": "node --import=tsx --watch src/index.ts",
    "start:prod": "node dist/index.js",
    "lint": "biome lint --fix",
    "format": "biome format --write",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "check": "pnpm lint && pnpm format && pnpm build && pnpm test",
    "db:generate": "prisma generate --schema=./prisma/schema.prisma",
    "db:push": "prisma db push --schema=./prisma/schema.prisma",
    "db:pull": "prisma db pull --schema=./prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
    "db:studio": "prisma studio --schema=./prisma/schema.prisma"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "7.3.0",
    "@supabase/supabase-js": "^2.49.4",
    "argon2": "^0.43.1",
    "cors": "2.8.5",
    "dotenv": "16.5.0",
    "express": "5.1.0",
    "express-rate-limit": "7.5.0",
    "helmet": "8.1.0",
    "http-status-codes": "2.3.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "9.6.0",
    "pino-http": "10.4.0",
    "swagger-ui-express": "5.0.1",
    "zod": "3.24.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@prisma/client": "^6.13.0",
    "@types/argon2": "^0.15.4",
    "@types/cors": "2.8.18",
    "@types/express": "5.0.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/supertest": "6.0.3",
    "@types/swagger-ui-express": "4.1.8",
    "@vitest/coverage-v8": "3.1.3",
    "pino-pretty": "13.0.0",
    "prisma": "^6.13.0",
    "supertest": "7.1.0",
    "tsup": "8.4.0",
    "tsx": "4.19.4",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.3"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "outDir": "dist",
    "format": [
      "esm",
      "cjs"
    ],
    "target": "es2020",
    "sourcemap": true,
    "clean": true,
    "dts": true,
    "splitting": false,
    "skipNodeModulesBundle": true
  },
  "packageManager": "pnpm@10.10.0"
}
